<h1>Listing news_secondaries</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Content</th>
    <th>Newscategory</th>
    <th>Author</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news_secondaries.each do |news_secondary| %>
  <tr>
    <td><%= news_secondary.title %></td>
    <td><%= news_secondary.description %></td>
    <td><%= news_secondary.content %></td>
    <td><%= news_secondary.newscategory_id %></td>
    <td><%= news_secondary.author %></td>
    <td><%= news_secondary.date %></td>
    <td><%= link_to 'Show', news_secondary %></td>
    <td><%= link_to 'Edit', edit_news_secondary_path(news_secondary) %></td>
    <td><%= link_to 'Destroy', news_secondary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

