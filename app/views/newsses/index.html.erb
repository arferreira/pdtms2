<h1>Listing newsfs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Content</th>
    <th>Author</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @newsses.each do |newss| %>
  <tr>
    <td><%= newss.title %></td>
    <td><%= newss.description %></td>
    <td><%= newss.content %></td>
    <td><%= newss.author %></td>
    <td><%= newss.date %></td>
    <td><%= image_tag newss.imagenews_url(:thumb) %></td>
    <td><%= link_to 'Show', newss %></td>
    <td><%= link_to 'Edit', edit_newsses_path(newss) %></td>
    <td><%= link_to 'Destroy', newss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Newsf', new_newsses_path %>
