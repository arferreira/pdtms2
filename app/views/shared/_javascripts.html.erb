    <script type="text/javascript">
        /* <![CDATA[ */
        $(function () {
      new UISearch( document.getElementById( 'sb-search' ) );
            //------TABS-------------
            $("#tab").organicTabs({
                "speed": 200
            });
      $('.quotes').quovolver({
        children : 'li',
        transitionSpeed : 300,
        autoPlay : true,
        equalHeight : false,
        navPosition : 'above',
        navPrev         : true,
        navNext         : true,
        navPrevText         : '&laquo;',
        navNextText         : '&raquo;',
        navNum          : false,
        navTextContent : 'Quote @a of @b'
      });
            var map = new GMaps({
                el: '#basic_map',
                lat: 14.6970,
                lng: 121.1220,
                zoom: 17,
                zoomControl : true,
                zoomControlOpt: {
                    style : 'SMALL',
                    position: 'TOP_LEFT'
                },
                panControl : false,
              });
            
                map.addMarker({
                  lat: 14.6970,
                  lng: 121.1220,
                  icon: "images/mapicon.png",
                  title: 'Marker with InfoWindow',
                  infoWindow: {
                    content: '<p><strong>PDT</strong><br>Mato Grosso do Sul</p>'
                  }
                });
            
              var styles = [
                {
                  stylers: [
                    { hue: "#eb6767" },
                    { saturation: -20 }
                  ]
                }, {
                    featureType: "road",
                    elementType: "geometry",
                    stylers: [
                        { lightness: 100 },
                        { visibility: "simplified" }
                  ]
                }, {
                    featureType: "road",
                    elementType: "labels",
                    stylers: [
                        { visibility: "off" }
                  ]
                }
              ];
                    
              map.addStyle({
                  styledMapName:"Styled Map",
                  styles: styles,
                  mapTypeId: "map_style"  
              });
              
              map.setStyle("map_style");
        });
        /* ]]> */
    </script>