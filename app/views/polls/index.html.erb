<h1>Listing polls</h1>

<table>
  <tr>
    <th>Questionmain</th>
    <th>Oneanswer</th>
    <th>Twoanswer</th>
    <th>Threeanswer</th>
    <th>Fouranswer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @polls.each do |poll| %>
  <tr>
    <td><%= poll.questionmain %></td>
    <td><%= poll.oneanswer %></td>
    <td><%= poll.twoanswer %></td>
    <td><%= poll.threeanswer %></td>
    <td><%= poll.fouranswer %></td>
    <td><%= link_to 'Show', poll %></td>
    <td><%= link_to 'Edit', edit_poll_path(poll) %></td>
    <td><%= link_to 'Destroy', poll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Poll', new_poll_path %>
